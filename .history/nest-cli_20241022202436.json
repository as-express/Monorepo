{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/movie/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/movie/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/movie",
  "projects": {
    "movie": {
      "type": "application",
      "root": "apps/movie",
      "entryFile": "main",
      "sourceRoot": "apps/movie/src",
      "compilerOptions": {
        "tsConfigPath": "apps/movie/tsconfig.app.json"
      }
    },
    "movie-app": {
      "type": "application",
      "root": "apps/movie-app",
      "entryFile": "main",
      "sourceRoot": "apps/movie-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/movie-app/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "usr": {
      "type": "application",
      "root": "apps/usr",
      "entryFile": "main",
      "sourceRoot": "apps/usr/src",
      "compilerOptions": {
        "tsConfigPath": "apps/usr/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "year": {
      "type": "application",
      "root": "apps/year",
      "entryFile": "main",
      "sourceRoot": "apps/year/src",
      "compilerOptions": {
        "tsConfigPath": "apps/year/tsconfig.app.json"
      }
    },
    "genre": {
      "type": "application",
      "root": "apps/genre",
      "entryFile": "main",
      "sourceRoot": "apps/genre/src",
      "compilerOptions": {
        "tsConfigPath": "apps/genre/tsconfig.app.json"
      }
    }
  }
}